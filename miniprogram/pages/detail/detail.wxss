/* pages/detail/detail.wxss */
@import "../home/skin.wxss";
@import "../me/text.wxss";
.title {
    font-weight: 900;
}   
.main {
    background-color: #fff;
}

.btn {
    background: #1296DB;
    color: white;
    border-radius: 20rpx;
}

.operateBar {
    position: fixed;
    bottom: 16rpx;
    width: 100%;
    height: 100rpx;
    display: inline-flex;
    justify-content: space-around;
}
.operateItem {
    width: 100rpx;
    height: 100rpx;
}

.operateItem image {
    width: 50rpx;
    height: 50rpx;
}